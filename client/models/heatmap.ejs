<div id="hm1" style="display: inline-block;"></div>
<div id="hm2" style="display: inline-block;"></div>
<div id="hm3" style="display: inline-block;"></div>
<div id="hm4" style="display: inline-block;"></div>

<script>
    utils.seed(1);
    const environment = new Environment();

    for (let i = 0; i < 200; i++) {
        environment.addAgent(new Agent({
            a: utils.uniform(),
            b: utils.uniform(),
            c: utils.gaussian(50, 20),
            d: utils.gaussian(75, 20)
        }));
    }

    const heatmap1 = new Heatmap(environment, {
        x: 'a',
        y: 'b'
    });

    heatmap1.mount('#hm1');

    const heatmap2 = new Heatmap(environment, {
        from: 'blue',
        to: 'yellow',
        x: {
            key: 'a',
            buckets: 14,
            min: -0.2,
            max: 1.2,
        },
        y: {
            key: 'b',
            buckets: 14,
            min: -0.2,
            max: 1.2
        }
    });

    heatmap2.mount('#hm2');

    const heatmap3 = new Heatmap(environment, {
        from: 'green',
        to: '#ffe',
        height: 200,
        width: 450,
        x: {
            key: 'c',
            max: 100
        },
        y: {
            key: 'd',
            max: 100
        }
    });

    heatmap3.mount('#hm3');

    const heatmap4 = new Heatmap(environment, {
        width: 800,
        x: {
            key: 'c',
            buckets: 8,
            max: 100
        },
        y: {
            key: 'd',
            buckets: 8,
            min: 50,
            max: 150
        }
    });

    heatmap4.mount('#hm4');

    environment.tick();
</script>